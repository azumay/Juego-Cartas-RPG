function tirarDados6() {
		    let valor = [0, 0, 0, 0, 0, 0];

		    for (let i = 0; i < valor.length; i++) {
		      valor[i] = parseInt(Math.random() * 15 + 3);
		    }
		    return valor;
		  }


var punts = [];

